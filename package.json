{
  "name": "maxine-discovery",
  "version": "1.0.0",
  "description": "Maxine is a service discovery and a registry server for all the running nodes with gargantua client dependency.",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "prod": "command -v taskset >/dev/null 2>&1 && taskset -c 0 node --max-old-space-size=4096 --optimize-for-size index.js || node --max-old-space-size=4096 --optimize-for-size index.js",
    "dev": "nodemon index.js --env dev",
    "test": "npm run unit-test",
    "lint": "eslint src/**/*.js index.js",
    "lint:fix": "eslint src/**/*.js index.js --fix",
    "format": "prettier --write src/**/*.js index.js",
    "maxibuild": "npx pkg -t node14-linux-arm64,node14-macos-arm64,node14-win-arm64 --out-path artifacts/build/ .",
    "unit-test": "HIGH_PERFORMANCE_MODE=false ULTRA_FAST_MODE=false LIGHTNING_MODE=false WEBSOCKET_ENABLED=true npx mocha --env dev --timeout 10000 --exit",
    "genreports": "nyc --reporter=html --reporter=lcov npm run unit-test",
    "coverage": "npm run genreports && npx codecov -t $CODECOV_TOKEN",
    "load-test": "chmod +x bin/k6 && ULTRA_FAST_MODE=true RATE_LIMIT_ENABLED=false node index.js & sleep 2 && bin/k6 run src/test/load-test/performance-test.js"
  },
  "keywords": [
    "maxine",
    "discovery-server",
    "naming-server",
    "registry-server",
    "service-registry"
  ],
  "author": "Vrushank Patel",
  "license": "MIT",
  "bin": "index.js",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.25",
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "@kubernetes/client-node": "^1.3.0",
    "@node-saml/passport-saml": "^5.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.30.1",
    "@opentelemetry/exporter-zipkin": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "activedirectory": "^0.7.2",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "dns2": "^2.1.0",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.17.3",
    "fast-json-stringify": "^6.0.1",
    "geoip-lite": "^1.4.10",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^6.0.6",
    "hashring": "^3.2.0",
    "heapdump": "^0.3.15",
    "http-proxy": "^1.18.1",
    "joi": "^18.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.2",
    "minimist": "^1.2.8",
    "mqtt": "^5.3.4",
    "p-limit": "^7.1.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.13",
    "subscriptions-transport-ws": "^0.11.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "ws": "^8.16.0"
  },
  "nodemonConfig": {
    "ext": "js,properties,yaml"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "codecov": "^3.8.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^11.7.2",
    "nodemon": "^3.1.10",
    "nyc": "^15.1.0",
    "prettier": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VrushankPatel/Maxine"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "**/app-builder.js",
      "**/route-builder.js",
      "**/logging"
    ],
    "excludeArchived": []
  },
  "pkg": {
    "assets": [
      "node_modules/**/*.css",
      "client/**/*"
    ]
  }
}
